{
  "name": "js-app-kickstarter",
  "version": "1.0.0",
  "author": "Szymon Dudziak",
  "private": true,
  "description": "Node-express kickstarter written in typescript",
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist node_modules ./.bin; npm i",
    "postbuild": "ln -s node_modules/.bin ./.bin; gulp",
    "start": "nodemon -L dist/bootstrap.js",
    "start:debug": "node --inspect dist/bootstrap.js"
  },
  "keywords": [
    "typescript",
    "node",
    "express",
    "socketIO",
    "SOLID",
    "dependency injection"
  ],
  "engines": {
    "node": ">=7.9.0",
    "typescript": "2.3.1",
    "npm": ">=4.2.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/test/**"
    ],
    "coverageReporters": [
      "json",
      "lcov"
    ]
  },
